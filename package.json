  "dependencies": {
    "date-fns": "^1.29.0",
    "react": "^16.4.1",
    "react-calendar": "^2.16.0",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
